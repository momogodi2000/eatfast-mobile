# EatFast Mobile App Environment Configuration
# Copy this file to .env and update the values

# API Configuration
API_BASE_URL=https://eat-fast-backend-express-js.onrender.com/api
SOCKET_URL=https://eat-fast-backend-express-js.onrender.com

# Build Configuration
FLUTTER_BUILD_MODE=release
FLUTTER_TARGET_PLATFORM=android

# App Configuration
APP_NAME=EatFast
APP_VERSION=1.0.0
BUILD_NUMBER=1

# Firebase Configuration (for push notifications)
FIREBASE_PROJECT_ID=eatfast-cameroon
FIREBASE_APP_ID=your-firebase-app-id
FIREBASE_API_KEY=your-firebase-api-key

# Google Services (for OAuth and Maps)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key
GOOGLE_OAUTH_CLIENT_ID=your-google-oauth-client-id

# Apple Services (for OAuth)
APPLE_OAUTH_CLIENT_ID=your-apple-oauth-client-id

# Development Configuration
DEV_API_BASE_URL=http://localhost:3000/api
DEV_SOCKET_URL=http://localhost:3000

# Testing Configuration
TEST_API_BASE_URL=https://eat-fast-backend-express-js.onrender.com/api
TEST_SOCKET_URL=https://eat-fast-backend-express-js.onrender.com

# Deployment Configuration
DEPLOY_TARGET=production
DEPLOY_PLATFORM=android